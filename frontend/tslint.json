{
    "defaultSeverity": "warning",
    "extends": "tslint-config-airbnb",
    "rules": {
        "ter-indent": [ true, 4 ],
        "import-name": false,
        "function-name": false,
        "no-unused-variable": false,
        "object-literal-key-quotes": false,
        "semicolon": [true, "always"],
        "no-parameter-reassignment": false
    }
}
