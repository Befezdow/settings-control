{
  "name": "frontend_js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "chart.js": "^2.7.3",
    "chartjs-plugin-zoom": "^0.6.6",
    "font-awesome": "^4.7.0",
    "js-logger": "^1.6.0",
    "luxon": "^1.9.0",
    "nconf": "^0.10.0",
    "v-toaster": "^1.0.3",
    "vue": "^2.5.18",
    "vue-chartjs": "^3.4.0",
    "vue-datetime": "^1.0.0-beta.8",
    "vue-js-modal": "^1.3.27",
    "vue-json-editor": "^1.2.3",
    "vue-moment": "^4.0.0",
    "vue-nav-tabs": "^0.5.7",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.14.0",
    "vue-yandex-maps": "^0.8.3",
    "vuex": "^3.0.1",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "semi": "off",
      "indent": [
        "warn",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "space-before-function-paren": "off",
      "no-trailing-spaces": "off",
      "vue/no-reserved-keys": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
